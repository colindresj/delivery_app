<div class="panel">
  <div id="form-content-region"></div>
  <% if @footer: %>
    <footer class="clearfix">
      <ul class="inline-list">
        <% for button in @buttons: %>
          <li>
            <button class="<%= button.classNames %>" data-form-button="<%= button.type %>"><%= button.text %></button>
          </li>
        <% end %>
      </ul>
    </footer>
  <% end %>
</div>
